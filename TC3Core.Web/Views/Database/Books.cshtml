@model BookIndexViewModel
@{
    ViewBag.PageTitle = "Database - Books";
    ViewBag.Title = "Books";
}
<table>
    @foreach (var item in Model.Books)
    {
        <tr>
            <td><a asp-action="Details" asp-route-id="@item.ID">@item.Title</a></td>
            <td><a asp-page="/Books/Edit" asp-route-id="@item.ID">Edit</a></td>
        </tr>
    }
</table>

@*@foreach (var decal in Model.Decals)
